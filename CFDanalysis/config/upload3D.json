{
    "displayName":"3D Building Geometry",
    "internalName":"upload3D",
    "description":"This model describes \nthe air flow around a building\n using an uploaded 3D geometry.",
    "icon":"upload3D.png",
    "sequence":["mesh","sim"],
    "stages":{
        "mesh":{
            "displayName":"Mesh",
            "app":"cwe-serial",
            "app_input":"geoFile",
            "groups":[
                "Building Geometry",
                "Mesh Density",
                "Subdomains",
                "Boundaries"
            ],
            "results":[
                {
                    "displayName":"Mesh Agave Log",
                    "type":"text",
                    "file":"logs/.agave.log"
                },
                {
                    "displayName":"Mesh Error Log",
                    "type":"text",
                    "file":"logs/cwe-serial.err"
                },
                {
                    "displayName":"Mesh Output Log",
                    "type":"text",
                    "file":"logs/cwe-serial.out"
                },
                {
                    "displayName":"Visualize 3D Mesh",
                    "type":"GLmesh3D"
                }

            ]
        },
        "sim":{
            "displayName":"Simulation",
            "app":"cwe-parallel",
            "groups":[
                "Simulation Control",
                "Advanced"
            ],
            "results":[
                {
                    "displayName":"Sim Agave Log",
                    "type":"text",
                    "file":"logs/.agave.log"
                },
                {
                    "displayName":"Sim Error Log",
                    "type":"text",
                    "file":"logs/cwe-parallel.err"
                },
                {
                    "displayName":"Sim Output Log",
                    "type":"text",
                    "file":"logs/cwe-parallel.out"
                },
                {
                    "displayName":"OpenFOAM Log",
                    "type":"text",
                    "file":"logs/sim.log"
                }
            ]
        }
    },
    "groups":{
        "Building Geometry":{
            "displayName":"Building Geometry",
            "vars":[
                "geoChoose",
                "geoFile"
                ],
            "image":"placeholderIcon.png"
        },
        "Mesh Density":{
            "displayName":"Mesh Density",
            "vars":[
                "inPad",
                "outPad",
                "lowYPad",
                "highYPad",
                "lowZPad",
                "highZPad",
                "meshDensity",
                "meshDensityFar"
            ]
        },
        "Subdomains":{
            "displayName":"Subdomains",
            "vars":[
                "innerDomains",
                "inPadDom1",
                "outPadDom1",
                "lowYDom1",
                "highYDom1",
                "lowZDom1",
                "highZDom1",
                "meshDensityDom1",
                "inPadDom2",
                "outPadDom2",
                "lowYDom2",
                "highYDom2",
                "lowZDom2",
                "highZDom2",
                "meshDensityDom2",
                "inPadDom3",
                "outPadDom3",
                "lowYDom3",
                "highYDom3",
                "lowZDom3",
                "highZDom3",
                "meshDensityDom3"
            ]
        },
        "Boundaries":{
            "displayName":"Boundaries",
            "vars":[
                "lowYPlane",
                "highYPlane",
                "lowZPlane",
                "highZPlane"
            ]
        },
        "Simulation Control":{
            "displayName":"Simulation Control",
            "vars":[
                "endTime",
                "deltaT",
                "velocity",
                "nu"
            ]
        },
        "Advanced":{
            "displayName":"Advanced",
            "vars":[
                "turbModel",
                "useForceCoeffs",
                "useFieldUmean",
                "useFieldPmean",
                "pisoCorrectors",
                "pisoNonOrthCorrect",
                "turbintensity"
            ]
        }
    },
    "vars":{
        "geoChoose":{
            "displayName":"Select Geometry File",
            "type":"choose",
            "default":"cwe_cube.json",
            "options":{
                "cwe_cube.json": "10x10x10m cube",
                "cwe_cone.json": "10x10x10m cone",
                "uploaded": "Uploaded File"
            }
        },
        "geoFile":{
            "displayName":"Uploaded File",
            "type":"file",
            "showCondition":"$geoChoose=uploaded"
        },
        "endTime":{
            "displayName":"Simulation Duration",
            "type":"std",
            "default":"1.0",
            "unit":"s",
            "precision":"2",
            "sign":"+"
        },
        "deltaT":{
            "displayName":"Simulation Time Step",
            "type":"std",
            "default":"0.1",
            "unit":"s",
            "precision":"6",
            "sign":"+"
        },
        "velocity":{
            "displayName":"Inflow Velocity",
            "type":"std",
            "default":"1",
            "unit":"m/s",
            "precision":"6",
            "sign":"+"
        },
        "nu":{
            "displayName":"Kinematic Viscosity",
            "type":"std",
            "default":"1.48e-05",
            "unit":"m^2/s",
            "precision":"9",
            "sign":"+"
        },
        "inPad":{
            "displayName":"Domain Length (Inlet)",
            "type":"std",
            "default":"8",
            "unit":"times max size of object",
            "precision":"1",
            "sign":"+"
        },
        "outPad":{
            "displayName":"Domain Length (Outlet)",
            "type":"std",
            "default":"20",
            "unit":"times max size of object",
            "precision":"1",
            "sign":"+"
        },
        "lowYPad":{
            "displayName":"Domain Length (-Y)",
            "type":"std",
            "default":"8",
            "unit":"times max size of object",
            "precision":"1",
            "sign":"+"
        },
        "highYPad":{
            "displayName":"Domain Length (+Y)",
            "type":"std",
            "default":"8",
            "unit":"times max size of object",
            "precision":"1",
            "sign":"+"
        },
        "lowZPad":{
            "displayName":"Domain Length (-Z)",
            "type":"std",
            "default":"0",
            "unit":"times max size of object",
            "precision":"1",
            "sign":"+"
        },
        "highZPad":{
            "displayName":"Domain Length (+Z)",
            "type":"std",
            "default":"8",
            "unit":"times max size of object",
            "precision":"1",
            "sign":"+"
        },
        "meshDensity":{
            "displayName":"Grid Size (on the bluff body)",
            "type":"std",
            "default":"3",
            "unit":"m",
            "precision":"2",
            "sign":"+"
        },
        "meshDensityFar":{
            "displayName":"Grid Size (on the outer bound)",
            "type":"std",
            "default":"10",
            "unit":"m",
            "precision":"2",
            "sign":"+"
        },
        "lowYPlane":{
            "displayName":"Boundary Condition (Y-)",
            "type":"choose",
            "default":"SYM_PLANE",
            "options":{
                "SYM_PLANE": "Symmetry Plane",
                "WALL": "Wall"
            }
        },
        "highYPlane":{
            "displayName":"Boundary Condition (Y+)",
            "type":"choose",
            "default":"SYM_PLANE",
            "options":{
                "SYM_PLANE": "Symmetry Plane",
                "WALL": "Wall"
            }
        },
        "lowZPlane":{
            "displayName":"Boundary Condition (Z-)",
            "type":"choose",
            "default":"SYM_PLANE",
            "options":{
                "SYM_PLANE": "Symmetry Plane",
                "WALL": "Wall"
            }
        },
        "highZPlane":{
            "displayName":"Boundary Condition (Z+)",
            "type":"choose",
            "default":"SYM_PLANE",
            "options":{
                "SYM_PLANE": "Symmetry Plane",
                "WALL": "Wall"
            }
        },
        "useForceCoeffs":{
            "displayName":"Compute Force Coefficients?",
            "type":"bool",
            "default": true
        },
        "useFieldUmean":{
            "displayName":"Compute Mean Velocity Field?",
            "type":"bool",
            "default": false
        },
        "useFieldPmean":{
            "displayName":"Compute Mean Pressure Field?",
            "type":"bool",
            "default": false
        },
        "turbModel":{
            "displayName":"Turbulence Model",
            "type":"choose",
            "default":"laminar",
            "options":{
                "laminar":"Laminar Flow model",
                "SpalartAllmaras":"S-A one equation model (RANS)",
                "kEpsilon":"k-epsilon model (RANS)",
                "Smagorinsky":"Smagorinsky turbulence model (LES)",
                "dynOneEqEddy":"Dynamic One Equation model (LES)"
            }
        },
        "turbintensity":{
            "displayName":"Turbulence Intensity",
            "type":"std",
            "default":"0.1",
            "precision":"2",
            "sign":"+",
            "showCondition":"$turbModel!laminar"
        },
        "pisoCorrectors":{
            "displayName":"Number of PISO Correctors",
            "type":"std",
            "default":"2",
            "precision":"int",
            "sign":"+0"
        },
        "pisoNonOrthCorrect":{
            "displayName":"Number of non-orthogonal Correctors",
            "type":"std",
            "default":"1",
            "precision":"int",
            "sign":"+0"
        },
        "innerDomains":{
            "displayName":"Number of Subdomains",
            "type":"choose",
            "default":"0",
            "options":{
                "0":"0",
                "1":"1",
                "2":"2",
                "3":"3"
            }
        },
        "inPadDom1":{
            "displayName":"Subdomain 1 Length (Inlet)",
            "type":"std",
            "default":"2",
            "unit":"times max size of object",
            "precision":"1",
            "sign":"+",
            "showCondition":"$innerDomains>=1"
        },
        "outPadDom1":{
            "displayName":"Subdomain 1 Length (Outlet)",
            "type":"std",
            "default":"4",
            "unit":"times max size of object",
            "precision":"1",
            "sign":"+",
            "showCondition":"$innerDomains>=1"
        },
        "lowYDom1":{
            "displayName":"Subdomain 1 Length (Y-)",
            "type":"std",
            "default":"2",
            "unit":"times max size of object",
            "precision":"1",
            "sign":"+",
            "showCondition":"$innerDomains>=1"
        },
        "highYDom1":{
            "displayName":"Subdomain 1 Length (Y+)",
            "type":"std",
            "default":"2",
            "unit":"times max size of object",
            "precision":"1",
            "sign":"+",
            "showCondition":"$innerDomains>=1"
        },
        "lowZDom1":{
            "displayName":"Subdomain 1 Length (Z-)",
            "type":"std",
            "default":"0",
            "unit":"times max size of object",
            "precision":"1",
            "sign":"+",
            "showCondition":"$innerDomains>=1"
        },
        "highZDom1":{
            "displayName":"Subdomain 1 Length (Z+)",
            "type":"std",
            "default":"2",
            "unit":"times max size of object",
            "precision":"1",
            "sign":"+",
            "showCondition":"$innerDomains>=1"
        },
        "meshDensityDom1":{
            "displayName":"Grid Size (Edge of Subdomain 2)",
            "type":"std",
            "default":"3",
            "unit":"m",
            "precision":"2",
            "sign":"+",
            "showCondition":"$innerDomains>=1"
        },
        "inPadDom2":{
            "displayName":"Subdomain 2 Length (Inlet)",
            "type":"std",
            "default":"4",
            "unit":"times max size of object",
            "precision":"1",
            "sign":"+",
            "showCondition":"$innerDomains>=2"
        },
        "outPadDom2":{
            "displayName":"Subdomain 2 Length (Outlet)",
            "type":"std",
            "default":"6",
            "unit":"times max size of object",
            "precision":"1",
            "sign":"+",
            "showCondition":"$innerDomains>=2"
        },
        "lowYDom2":{
            "displayName":"Subdomain 2 Length (Y-)",
            "type":"std",
            "default":"4",
            "unit":"times max size of object",
            "precision":"1",
            "sign":"+",
            "showCondition":"$innerDomains>=2"
        },
        "highYDom2":{
            "displayName":"Subdomain 2 Length (Y+)",
            "type":"std",
            "default":"4",
            "unit":"times max size of object",
            "precision":"1",
            "sign":"+",
            "showCondition":"$innerDomains>=2"
        },
        "lowZDom2":{
            "displayName":"Subdomain 2 Length (Z-)",
            "type":"std",
            "default":"0",
            "unit":"times max size of object",
            "precision":"1",
            "sign":"+",
            "showCondition":"$innerDomains>=2"
        },
        "highZDom2":{
            "displayName":"Subdomain 2 Length (Z+)",
            "type":"std",
            "default":"4",
            "unit":"times max size of object",
            "precision":"1",
            "sign":"+",
            "showCondition":"$innerDomains>=2"
        },
        "meshDensityDom2":{
            "displayName":"Grid Size (Edge of Subdomain 2)",
            "type":"std",
            "default":"3",
            "unit":"m",
            "precision":"2",
            "sign":"+",
            "showCondition":"$innerDomains>=2"
        },
        "inPadDom3":{
            "displayName":"Subdomain 3 Length (Inlet)",
            "type":"std",
            "default":"6",
            "unit":"times max size of object",
            "precision":"1",
            "sign":"+",
            "showCondition":"$innerDomains>=3"
        },
        "outPadDom3":{
            "displayName":"Subdomain 3 Length (Outlet)",
            "type":"std",
            "default":"8",
            "unit":"times max size of object",
            "precision":"1",
            "sign":"+",
            "showCondition":"$innerDomains>=3"
        },
        "lowYDom3":{
            "displayName":"Subdomain 3 Length (Y-)",
            "type":"std",
            "default":"6",
            "unit":"times max size of object",
            "precision":"1",
            "sign":"+",
            "showCondition":"$innerDomains>=3"
        },
        "highYDom3":{
            "displayName":"Subdomain 3 Length (Y+)",
            "type":"std",
            "default":"6",
            "unit":"times max size of object",
            "precision":"1",
            "sign":"+",
            "showCondition":"$innerDomains>=3"
        },
        "lowZDom3":{
            "displayName":"Subdomain 3 Length (Z-)",
            "type":"std",
            "default":"0",
            "unit":"times max size of object",
            "precision":"1",
            "sign":"+",
            "showCondition":"$innerDomains>=3"
        },
        "highZDom3":{
            "displayName":"Subdomain 3 Length (Z+)",
            "type":"std",
            "default":"6",
            "unit":"times max size of object",
            "precision":"1",
            "sign":"+",
            "showCondition":"$innerDomains>=3"
        },
        "meshDensityDom3":{
            "displayName":"Grid Size (Edge of Subdomain 3)",
            "type":"std",
            "default":"3",
            "unit":"m",
            "precision":"2",
            "sign":"+",
            "showCondition":"$innerDomains>=3"
        }
    }
}
